

a.
#include<iostream>
using namespace std;

int main()
{
	int i,j;
	char l='A';
	for( i=1;i<=5;i++)
	{
		for(j=1;j<=5;j++)
		{
			if(j<=i)
			{
				cout<<" "<<l;
			}
			else{
				cout<<" ";
			}
		}
		   l++;
		cout<<endl;
	}
}




b.
#include<iostream>
using namespace std;

int main()
{
	int i,j;
	char l;
	for( i=1;i<=5;i++)
	{
		l='A';
		for(j=1;j<=5;j++)
		{
			if(j<=i)
			{
				cout<<" "<<l;
			}
			else{
				cout<<" ";
			}
			l++;
		}
		   
		cout<<endl;
	}
}



c.
#include<iostream>
using namespace std;

int main()
{
	int i,j,a;
	
	for( i=1;i<=5;i++)
	{
		a=1;
		for(j=1;j<=5;j++)
		{
			if(j<=6-i)
			{
				cout<<" "<<a;
				a++;
			}
			else{
				cout<<" ";
			}
			
		}
		   
		cout<<endl;
	}
}





d.
#include<iostream>
using namespace std;

int main()
{
	int i,j,a=1;
	
	for( i=1;i<=4;i++)
	{
		
		for(j=1;j<=5;j++)
		{
		
			if(j<=i)
			{
				cout<<" "<<a;
				a++;
			}
			else{
				cout<<" ";
			}
			
		}
		   
		cout<<endl;
	}
}






e.
#include<iostream>
using namespace std;

int main()
{
	int i,j;
	char l='A';
	
	for( i=1;i<=5;i++)
	{
		
		for(j=1;j<=5;j++)
		{
		
			if(j<=i)
			{
				cout<<" "<<l;
				l++;
			}
			else{
				cout<<" ";
			}
			
		}
		   
		cout<<endl;
	}
}





f.
#include<iostream>
using namespace std;

int main()
{
	int i,j,k;
	for( i=1;i<=5;i++)
	{
		k=1;
		for(j=1;j<=9;j++)
		{
			if(j>=6-i&&j<=4+i&&k)
			{
				cout<<"*";
				k=0;
			}
			else{
				cout<<" ";
				k=1;
			}
		}
		cout<<endl;
	}
}






g.
#include<iostream>
using namespace std;

int main()
{
	int i,j;
	
	for( i=1;i<=5;i++)
	{
		for(j=1;j<=5;j++)
		{
		
		if(j>=(6-i))
			{
				cout<<"*";
				
			}
			else{
				cout<<" ";
			}	
		}	   
		cout<<endl;
	}
}






h.
#include<iostream>
using namespace std;

int main()
{
	int i,j;
	
	for( i=1;i<=5;i++)
	{
		for(j=1;j<=5;j++)
		{
		
		if(j<=(6-i))
			{
				cout<<"*";
				
			}
			else{
				cout<<" ";
			}	
		}	   
		cout<<endl;
	}
}






i.
#include<iostream>
using namespace std;

int main()
{
	int i,j;
	
	for( i=1;i<=5;i++)
	{
		for(j=1;j<=5;j++)
		{
		
		if(j>=i)
			{
				cout<<"*";
				
			}
			else{
				cout<<" ";
			}	
		}	   
		cout<<endl;
	}
}






j.
#include<iostream>
using namespace std;

int main()
{
	int i,j;
	
	for( i=1;i<=5;i++)
	{
		for(j=1;j<=9;j++)
		{
		
		if(j>=6-i&&j<=4+i)
			{
				cout<<"*";
				
			}
			else{
				cout<<" ";
			}	
		}	   
		cout<<endl;
	}
}
